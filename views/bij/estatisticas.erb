                <canvas id="chartTotalAcertosErros" width="500" height="300"></canvas>
                <div id="legendaTotalAcertosErros" style="position:absolute"></div>
              </div><!-- /.box-body -->
            </div>
          </div>

          <div class="col-md-6">
            <div class="box box-solid box-primary">
              <div class="box-header with-border">
