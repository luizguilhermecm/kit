                  <labeL for="banca_d">Banca:</labeL>
                  <select name="banca_d" id="banca_d" onchange="carregar_disciplinas()"><%= @bancas.join("") %></select>
                  &nbsp;&nbsp;&nbsp;
                  <labeL for="ano_d">Ano:</labeL>
                  <select name="ano_d" id="ano_d" onchange="carregar_disciplinas()"><%= @anos.join("") %></select>
                  <br>
                  <labeL for="orgao_d">Órgão:</label>
                  <select name="orgao_d" id="orgao_d" onchange="carregar_disciplinas()"><%= @orgaos.join("") %></select>
                  <br>
                  <labeL for="prova_d">Prova:</label>
                  <select name="prova_d" id="prova_d" onchange="carregar_disciplinas()"><%= @provas.join("") %></select>
